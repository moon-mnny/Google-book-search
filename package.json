  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.19.4"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.20.1",
    "express": "^4.18.2",
    "if-env": "^1.0.4",
    "mongoose": "^5.9.6",
    "morgan": "^1.10.0"
  }
}
